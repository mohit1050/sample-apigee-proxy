<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<ProxyEndpoint name="kibana">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
</PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/kibana</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="kibana">
    <TargetEndpoint>kibana</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>