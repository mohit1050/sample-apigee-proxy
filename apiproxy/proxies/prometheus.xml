<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<ProxyEndpoint name="prometheus">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
</PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/prometheus</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="prometheus">
    <TargetEndpoint>prometheus</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>