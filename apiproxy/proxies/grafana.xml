<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<ProxyEndpoint name="grafana">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
</PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <HTTPProxyConnection>
    <BasePath>/grafana</BasePath>
  </HTTPProxyConnection>
  <RouteRule name="grafana">
    <TargetEndpoint>grafana</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>